#include <stdio.h>
#include <string.h>

int main() {
    char s1[1000], s2[1000], s3[2000];
    fgets(s1, 1000, stdin);
    fgets(s2, 1000, stdin);
    int n1 = strlen(s1), n2 = strlen(s2);
    if(n1 > 0 && s1[n1-1] == '\n') s1[--n1] = '\0';
    if(n2 > 0 && s2[n2-1] == '\n') s2[--n2] = '\0';
    if(n1 != n2) {
        printf("Not Rotation");
        return 0;
    }
    strcpy(s3, s1);
    strcat(s3, s1);
    if(strstr(s3, s2))
        printf("Rotation");
    else
        printf("Not Rotation");
    return 0;
}
