#include <stdio.h>

int main() {
    int n, target, i, first = -1, last = -1;
    int nums[1000];

    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &nums[i]);
    scanf("%d", &target);

    for(i = 0; i < n; i++)
        if(nums[i] == target) {
            first = i;
            break;
        }

    for(i = n - 1; i >= 0; i--)
        if(nums[i] == target) {
            last = i;
            break;
        }

    if(first == -1) printf("-1 -1");
    else printf("%d %d", first, last);

    return 0;
}
