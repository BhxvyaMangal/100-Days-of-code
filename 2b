#include<stdio.h>
int main(){
    float l,b,area,perimeter;
    printf("Length of rectangle");
    scanf("%f",&l);
    printf("breadth of rectangle");
    scanf("%f",&b);
    area=l*b;
    perimeter=2*(l+b);
    printf("Area of Rectangle is %f\n",area);
    printf("Perimeter of Rectangle is %f\n",perimeter);
    return 0;
}
