#include <stdio.h>

int main() {
    char str[1000];
    int i, j, k;

    fgets(str, 1000, stdin);

    for(i = 0; str[i] != '\0' && str[i] != '\n'; i++)
        for(j = i; str[j] != '\0' && str[j] != '\n'; j++) {
            for(k = i; k <= j; k++)
                printf("%c", str[k]);
            printf("\n");
        }

    return 0;
}

