//Write a program to find the LCM of two numbers.
#include <stdio.h>

int main() {
    int a, b, num1, num2, temp;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    num1 = a;
    num2 = b;

    while (num2 != 0) {
        temp = num2;
        num2 = num1 % num2;
        num1 = temp;
    }

    int gcd = num1;
    int lcm = (a * b) / gcd;

    printf("LCM of %d and %d is: %d\n", a, b, lcm);

    return 0;
}
