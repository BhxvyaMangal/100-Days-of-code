#include <stdio.h>

int main() {
    int n, i;
    int arr[1000], seen[1000] = {0};

    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if(seen[arr[i]] == 1) {
            printf("%d", arr[i]);
            return 0;
        }
        seen[arr[i]] = 1;
    }

    return 0;
}
