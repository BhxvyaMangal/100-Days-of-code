#include <stdio.h>

int main() {
    char str[1000];
    int i = 0, v = 0, c = 0;

    fgets(str, 1000, stdin);

    while(str[i] != '\0') {
        char ch = str[i];
        if((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z')) {
            if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||
               ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
                v++;
            else
                c++;
        }
        i++;
    }

    printf("%d %d", v, c);

    return 0;
}
