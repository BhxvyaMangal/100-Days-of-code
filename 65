#include <stdio.h>
#include <string.h>

int main() {
    char s[1000], t[1000];
    int freq[26] = {0};
    int i;

    fgets(s, 1000, stdin);
    fgets(t, 1000, stdin);

    int n1 = strlen(s);
    int n2 = strlen(t);
    if(n1 > 0 && s[n1-1] == '\n') s[--n1] = '\0';
    if(n2 > 0 && t[n2-1] == '\n') t[--n2] = '\0';

    if(n1 != n2) {
        printf("Not Anagram");
        return 0;
    }

    for(i = 0; i < n1; i++)
        freq[s[i] - 'a']++;

    for(i = 0; i < n2; i++)
        freq[t[i] - 'a']--;

    for(i = 0; i < 26; i++)
        if(freq[i] != 0) {
            printf("Not Anagram");
            return 0;
        }

    printf("Anagram");
    return 0;
}
